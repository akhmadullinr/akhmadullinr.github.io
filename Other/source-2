<script type="text/javascript">
    var under_mouse_object = 0;
    var last_focus = 0;
    var load_values = new Array();
    function formatItem(row) {
        return row[2].replace(/\r/g, "<br>\n");
    }
    function formatResult(row) {
        if (row[0] == "") return "";
        return row[1];
    }
    function fix_result(event, data, formatted) {
        var hidden = $(this).next().next();
        hidden.val(data[0]);
        hidden.change();
    }

    $().ready(function () {
                                                                                                if (document.getElementById("value3711")) load_values[3711] = document.getElementById("value3711").value; 
