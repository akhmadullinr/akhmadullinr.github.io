<style>
    textarea {
        /*transition: height 0.2s;
    -webkit-transition: height 0.2s;
    -moz-transition: height 0.2s;*/
        resize: none;
        font-size: 13px;
        /*font-family: Arial, Verdana, sans-serif;*/
    }

    .onfocus_border {
        padding-top: 3px;
        padding-bottom: 3px;
        padding-left: 3px;
        padding-right: 3px;
        min-height: 17px;
        border: 1px dotted #ffffff;
        cursor: text;
    }

    .textpad {
        word-wrap: break-word;
        white-space: normal;
    }

    .fast_edit_link {
        white-space: nowrap;
    }

    .phone_type:hover {
        background: transparent !important;
    }

    .phone_type img {
        background: transparent !important;
    }

    .phone_type img:hover {
        background: transparent !important;
