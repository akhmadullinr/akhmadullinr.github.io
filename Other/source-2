<script>
    function explode(delimiter, string) {
        var emptyArray = { 0: '' };
        if (arguments.length != 2
                || typeof arguments[0] == 'undefined'
                || typeof arguments[1] == 'undefined') {
            return null;
        }
        if (delimiter === ''
                || delimiter === false
                || delimiter === null) {
            return false;
        }
        if (typeof delimiter == 'function'
                || typeof delimiter == 'object'
                || typeof string == 'function'
                || typeof string == 'object') {
            return emptyArray;
        }
        if (delimiter === true) {
            delimiter = '1';
        }
        return string.toString().split(delimiter.toString());
    }
</script>
<script type="text/javascript" src="include/jquery-fixedTable/jquery.fixedTable.js"></script>
<script>
    window.onload = function() {
        setTimeout(function() {
            $('.blockMY').fixedTable();
        }, 100);
    }
</script>
