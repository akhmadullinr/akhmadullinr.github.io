<script type="text/javascript">
    var help_available = 1;
    
        
        var checksum_scripts = {  };

    $.widget("ui.dialog", $.extend({}, $.ui.dialog.prototype, {
        _title: function (title) {
            if (!this.options.title) {
                title.html("&#160;");
            } else {
                title.html(this.options.title);
            }
        }
    }));

    function check_old_css() {
        var t_css, i, f_name, t_text, t_md5, refresh_name = '';
        // Проверяем css
        for (i in document.styleSheets) {
            t_css = document.styleSheets[i];
            if ((t_css.href) && (typeof(t_css.href) != 'undefined')) {
                f_name = t_css.href.substr(22+1, 255);
                if (f_name == "") continue;
                if (f_name.substr(0, 5) == 'cache') continue;
                if (f_name) {
                    if (typeof(document.styleSheets[i].rules) != 'undefined')
                        if (typeof(t_text = document.styleSheets[i].rules[0]) == 'undefined') continue;
                        else
                            t_text = document.styleSheets[i].rules[0].selectorText;
                    else if (typeof(t_text = document.styleSheets[i].cssRules[0]) == 'undefined') continue;
                    else
                        t_text = document.styleSheets[i].cssRules[0].selectorText;
                    if (t_text == undefined) continue;
                    if (t_text.substr(0, 10) == '.checksum_') {
                        t_md5 = t_text.substr(10, 255);
                        if (checksum_scripts[f_name] && checksum_scripts[f_name] != t_md5) {
                            if (typeof(console) != undefined)
                                console.log('Old css file: ' + f_name);
                            refresh_name = f_name;
                        }
                    }
                }
            }
        }
        // Проверяем javascript
        if (typeof(checksum_scripts_current) != 'undefined')
            for (i in checksum_scripts_current) {
                if (checksum_scripts[i] && (checksum_scripts[i] != checksum_scripts_current[i])) {  // Контрольные суммы не совпадают
                    refresh_name = i;
                    if (typeof(console) != undefined)
                        console.log('Old js file: ' + i);
                }
            }
        if (refresh_name != '') { // Необходимо обновить скрипты
            jalert('Внимание! Программа Клиентская база была обновлена. Чтобы обновления вступили в силу, нажмите Ctrl+F5 и затем F5 (CMD+R, если Вы используете Mac OS) или очистите кэш браузера.');
        }
    }

    // закрытие напоминаний, включая плавное исчезновение и фоновое обновление данных (ajax)
    var eOpacity = 1;
    var interval = false;
    var update_tip_ajax = new XMLHTTP("update_tip.php");

    function closeTip(user_id, tip_show_id, line_id) {
        slideOff('tip_block_' + tip_show_id);
        eOpacity = 1;

        update_tip_ajax.method = "POST";
        update_tip_ajax.call("tip_show_id=" + tip_show_id + "&csrf=6RYQiq2uSijHG3PnKt5jPw==", ComRespTip);
    }
    function flagTip(tip_show_id) {
        var im = document.getElementById('top_tip_flag_' + tip_show_id);
        var sc = im.src;
        var new_state = 1;
        if (sc.indexOf('star.png') > 0) new_state = 0;
        if (new_state) im.src = 'images/star.png';
        else im.src = 'images/star_gray.png';
        update_tip_ajax.method = "POST";
        update_tip_ajax.call("tip_show_id=" + tip_show_id + "&change_flag=1&csrf=6RYQiq2uSijHG3PnKt5jPw==", ComRespTip);
    }

    function ComRespTip(resp) {

    }


    function slideOff(sElemId) {
        var delta = Math.floor(document.getElementById(sElemId).offsetHeight / 10);
        interval = window.setInterval("minus('" + sElemId + "', " + delta + ")", 50);
    }

    function minus(sElemId, delta) {
        nHeight = document.getElementById(sElemId).offsetHeight;
        nHeight = parseInt(nHeight);
        nHeight = nHeight - delta;
        if (nHeight > 0) {
            eOpacity = eOpacity - 0.1;
            document.getElementById(sElemId).style.height = nHeight + "px";
            setElementOpacity(sElemId, eOpacity);
            on_top_resize();
        }
        else {
            if (nHeight == '0') {
                document.getElementById(sElemId).style.display = "none";
                document.getElementById(sElemId).style.height = '1' + "px";
                window.clearInterval(interval);
                on_top_resize();
            }
            else {
                minus(sElemId, delta - 1);
            }
        }
    }

    function setElementOpacity(sElemId, nOpacity) {
        var opacityProp = getOpacityProperty();
        var elem = document.getElementById(sElemId);

        if (!elem || !opacityProp) return; // Если не существует элемент с указанным id или браузер не поддерживает ни один из известных функции способов управления прозрачностью

        if (opacityProp == "filter")  // Internet Exploder 5.5+
        {
            nOpacity *= 100;

            // Если уже установлена прозрачность, то меняем её через коллекцию filters, иначе добавляем прозрачность через style.filter
            var oAlpha = elem.filters['DXImageTransform.Microsoft.alpha'] || elem.filters.alpha;
            if (oAlpha) oAlpha.opacity = nOpacity;
            else elem.style.filter += "progid:DXImageTransform.Microsoft.Alpha(opacity=" + nOpacity + ")"; // Для того чтобы не затереть другие фильтры используем "+="
        }
        else // Другие браузеры
            elem.style[opacityProp] = nOpacity;
    }

    function getOpacityProperty() {
        if (typeof document.body.style.opacity == 'string') // CSS3 compliant (Moz 1.7+, Safari 1.2+, Opera 9)
            return 'opacity';
        else if (typeof document.body.style.MozOpacity == 'string') // Mozilla 1.6 и младше, Firefox 0.8
            return 'MozOpacity';
        else if (typeof document.body.style.KhtmlOpacity == 'string') // Konqueror 3.1, Safari 1.1
            return 'KhtmlOpacity';
        else if (document.body.filters && navigator.appVersion.match(/MSIE ([\d.]+);/)[1] >= 5.5) // Internet Exploder 5.5+
            return 'filter';

        return false; //нет прозрачности
    }

            var prev_cat =1;
    function open_calc(params, text_len) {
        if (text_len > 3000) { // Большие тексты открываем в окне на весь экран
            var left = 0;
            var top = 0;
            var width = screen.availWidth;
            var height = screen.availHeight;
            if (window.opera) { // чтобы окно открылось отдельной вкладкой
                width += 500;
                height += 500;
            }
        }
        else {
            var width = 800;
            var height = 600;
            var left = ( screen.width - width ) / 2;
            var top = ( screen.height - 600 ) / 2;
        }
        var t_v = window.open('edit_php.php?' + params, (params.replace(/&/g, "")).replace(/=/g, ""), 'top=' + top + ', left=' + left + ', menubar=0, resizable=1, scrollbars=0, status=0, toolbar=0, width=' + width + ', height=' + height);
        t_v.focus();
    }


    // Обработка перемотки окна

    // Получить css свойство объекта
    function getElementComputedStyle(elem, prop) {
        if (typeof elem != "object") elem = document.getElementById(elem);

        // external stylesheet for Mozilla, Opera 7+ and Safari 1.3+
        if (document.defaultView && document.defaultView.getComputedStyle) {
            if (prop.match(/[A-Z]/)) prop = prop.replace(/([A-Z])/g, "-$1").toLowerCase();
            return document.defaultView.getComputedStyle(elem, "").getPropertyValue(prop);
        }

        // external stylesheet for Explorer and Opera 9
        if (elem.currentStyle) {
            var i;
            while ((i = prop.indexOf("-")) != -1) prop = prop.substr(0, i) + prop.substr(i + 1, 1).toUpperCase() + prop.substr(i + 2);
            return elem.currentStyle[prop];
        }

        return "";
    }

    // Привести к intval
    function intval(mixed_var, base) {    // Get the integer value of a variable
        //
        // +   original by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
        var tmp;

        if (typeof( mixed_var ) == 'string') {
            tmp = parseInt(mixed_var);
            if (isNaN(tmp)) {
                return 0;
            } else {
                return parseInt(tmp.toString(base || 10));
            }
        } else if (typeof( mixed_var ) == 'number') {
            return parseInt(Math.floor(mixed_var));
        } else {
            return 0;
        }
    }

    // Функция для добавления обработчика событий
    // Сохраняем обработчки onsubmit для edit_form в отдельном массиве
    var edit_form_submits = new Array();
    function addHandler(object, event, handler, useCapture) {
        if (!object) return;
        if ((document.getElementById('edit_form')) && (object == document.getElementById('edit_form')) && (event == "onsubmit")) { // сохраняем события onsubmit формы edit_form
            edit_form_submits.push(handler);
        }

        if (object.addEventListener) {
            var t1;
            if (event.substr(0, 2).toLowerCase() == "on") t1 = event.substr(2, 1024); // убираем приставку on
            object.addEventListener(t1, handler, useCapture ? useCapture : false);
        } else if (object.attachEvent) {
            object.attachEvent(event, handler);
        } else jalert("Add handler is not supported");
    }

    
    var table_padding = 32;
    var buttons_h = 37;
    var table_header_h = 0;
    var right_white;
    var left_white;
    var top_header_div;
    var fields_content_table;
    var window_width;
    var window_height;
    var fields_buttons_table;
    var old_offs_x = -1;
    var old_offs_y = -1;
    var old_new_h = -1;
    var scroll_orientir = -1;
    var header_orientir = -1;
    var visible_popup = 0;
    var table_div;
    var rp_div;
    var page_loaded = 0;

    // Скролл окна
    function onMyScroll(event) {
        var offs_x = window.pageXOffset;
        var offs_y = window.pageYOffset;

        if (typeof(offs_x) != 'number') {
            offs_x = document.documentElement.scrollLeft;
            offs_y = document.documentElement.scrollTop;
        }

        if (offs_x != old_offs_x) {  // Ездит X позиция
            old_offs_x = offs_x;
            if (visible_popup) {
                move_table_headers(offs_x, offs_y);
            }
            else { // виден родной заголовок
                // меняем позицию у галочки в шапке таблицы
                if (fields_content_table) {
                    document.getElementById('header_0_div').style.top = (fields_content_table.offsetTop - offs_y) + "px";
                    document.getElementById('header_0_div').style.left = table_padding + "px";
                }
                // и ведеться скролл по оси х, переключаем вид в fixed режим шапку
                if (header_orientir != 0) {
                    // меняем позицию на фиксированную у шапки
                    if (top_header_div) {
                        document.getElementById('top_header_div').style.position = "fixed";
                        document.getElementById('top_header_div').style.left = "0px";
                        document.getElementById('top_header_div').style.top = old_new_h + "px";
                    }
                    header_orientir = 0;
                }
                ;
            }
            if (scroll_orientir != 0) {
                // меняем галочку в шапке таблицы на fixed
                if (fields_content_table) {
                    document.getElementById('header_0_div').style.position = "fixed";

                    // меняем позицию на фиксированную у div_table
                    table_div.style.position = "fixed";
                    table_div.style.left = table_padding + "px";
                    table_div.style.top = (fields_content_table.offsetTop - offs_y) + "px";
                }
                scroll_orientir = 0;
            }
            ;
            // меняем позицию закрепленного поля
                    }
        if (offs_y != old_offs_y) {  // Ездит Y позиция
            old_offs_y = offs_y;
            var h_top, b_h, new_h;
            if (fields_buttons_table) { // Страница с кнопками
                h_top = (-offs_y + top_header_div.offsetHeight);
                b_h = fields_buttons_table.offsetHeight;
                new_h = -offs_y;
                if (h_top < b_h) {
                    new_h = -offs_y + (b_h - h_top);
                    if (!visible_popup) { // зажимаем сверху
                        // шапку
                        document.getElementById('top_header_div').style.position = "fixed";
                        document.getElementById('top_header_div').style.left = "0px";
                        document.getElementById('top_header_div').style.top = (-top_header_div.offsetHeight + b_h) + "px";

                        // меняем позицию на fixed у галочки в шапке таблицы
                        if (fields_content_table) {
                            document.getElementById('header_0_div').style.position = "fixed";
                            document.getElementById('header_0_div').style.top = buttons_h + "px";
                            document.getElementById('header_0_div').style.left = table_padding + "px";
                        }
                        // зажимаем сверху ячейки таблицы
                        fix_table_headers(offs_x);
                        move_table_headers(offs_x, offs_y);
                        header_orientir = 1;
                    }
                    visible_popup = 1;
                }
                else {
                    if (visible_popup) {
                        free_table_headers();
                        header_orientir = 0;
                    }
                    visible_popup = 0;
                }
                ;
            }
            else { // Страница без кнопок
            }
            if (header_orientir != 1) {
                // меняем позицию на абсолютную у шапки

                if (top_header_div) {
                    document.getElementById('top_header_div').style.position = "absolute";
                    document.getElementById('top_header_div').style.left = offs_x + "px";
                    document.getElementById('top_header_div').style.top = "0px";
                }

                // меняем позицию на абсолютную у галочки в меню
                if (fields_content_table) {
                    document.getElementById('header_0_div').style.position = "absolute";
                    document.getElementById('header_0_div').style.top = fields_content_table.offsetTop + "px";
                    document.getElementById('header_0_div').style.left = (table_padding + offs_x) + "px";
                }
                header_orientir = 1;
            }
            ;

            if (scroll_orientir != 1) {
                // меняем позицию на абсолютную у div таблицы
                if (fields_content_table) {
                    table_div.style.position = "absolute";
                    table_div.style.left = (table_padding + offs_x) + "px";
                }
                scroll_orientir = 1;
            }
            ;
            if (fields_content_table) {
                table_div.style.top = fields_content_table.offsetTop + "px";
            }
            old_new_h = new_h;
        }
        return true;
    }

    // Загружена страница
    function pageloaded() {
        window.onresize = on_window_resize;
        on_window_resize();
                page_loaded = 1;
    }

    function move_table_headers(t_l, t_h) {
        var v_t, v_t2;
        if (fields_content_table) {
            document.getElementById('f_row').style.left = table_padding - t_l + "px";
            // меняем позицию у галочки в шапке таблицы
            document.getElementById('header_0_div').style.top = buttons_h + "px";
            document.getElementById('header_0_div').style.left = table_padding + "px";
        }
    }

    function fix_table_headers() {
        if (fields_content_table) {
            var v_t, v_t2;
            // устанавливаем тип блок
            document.getElementById('f_row').style.display = "block";
            document.getElementById('f_row').style.position = "fixed";
            document.getElementById('f_row').style.top = buttons_h + "px";
            document.getElementById('f_row').style.left = table_padding + "px";

            // выносим на уровень выше
            document.getElementById('f_row').style.zIndex = "1";

            // восстанавливаем высоту блока
            document.getElementById('header_t_0').style.height = table_header_h + "px";
        }
    }
    ;


    function free_table_headers() {
        if (fields_content_table) {
            document.getElementById('f_row').style.display = "";
            document.getElementById('f_row').style.position = "";
            document.getElementById('f_row').style.top = "";
            document.getElementById('f_row').style.left = ""
            document.getElementById('f_row').style.zIndex = 0;

            // восстанавливаем высоту блока
            document.getElementById('header_t_0').style.height = "0px";
        }
        ;
    }
    ;

    function fnShowProps(obj, objName) {
        var result = "";
        for (var i in obj) // обращение к свойствам объекта по индексу
            result += objName + "." + i + " = " + obj[i] + "<br />\n";
        document.write(result);
    }

    function on_table_load_fixing() {
        if (!fields_content_table) return;

        var v_t, v_t2, v_el;
        fields_content_table.style.tableLayout = "";

        // Запоминаем старые параметры
        table_header_h = document.getElementById('header_0').offsetHeight;
        document.getElementById('f_row_virtual').style.width = document.getElementById('f_row').offsetWidth + "px";
        document.getElementById('header_t_0').style.width = $('#header_0').offset().left + "px";
                        document.getElementById('header_t_912').style.width = document.getElementById('header_912').offsetWidth + "px";
                                document.getElementById('header_t_913').style.width = document.getElementById('header_913').offsetWidth + "px";
