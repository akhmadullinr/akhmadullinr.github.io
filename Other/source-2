            additional_params: function () {
                if (document.getElementById('value' +895)) return "&filter_value=" + document.getElementById('value' +895).value + "";
                return "";
            }  }).result(fix_result);
                if (document.getElementById("value443")) load_values[443] = document.getElementById("value443").value; 
