   return 0;
};

$(function() {
// Документ загружен
// Разрешаем редактировать строку
 $(".disbled_edit").removeClass("disbled_edit").addClass('edit');
});

function show_crypt_field(edit_mode) {
    jconfirm('<div align=center>Для доступа к данному полю необходимо ввести ключ:<br><br><input type=password onchange="$.cookie(\'crypt_password\', this.value)"></div>',
        function() {
            if (edit_mode) {
                location.href += "&edit_mode";
            } else {
                location.reload();
            }
        },
        function() {
            $.cookie('crypt_password', null);
        }
    );
}

</script>
<script language="JavaScript" type="text/javascript">
id_fields = ['912','913','914','915','3711','3721','435','1047','441','442','3731','443','895','438','772','552','444','770','445','446','884','3700','1460','1470','1480','439','440','3340','1056','1057','1065','1061','1058','1059','1060','1062','1063','1064','3810','3741','3751','3761','3771','3781','3791','1088','2930','2940'];
function addfile(field_id)
{
  if (document.getElementById('add_file'+field_id).value)
    {
      document.getElementById('edit_mode').value = 1;
      save_line();
	  var add_files_flag = 1;//флаг не дает windowCloseHandler поставить куку autosave
    }
    else
    {
      jalert('Перед добавлением выберите загружаемый файл нажатием на кнопку Обзор');
    }
}

function delfile(field_id, file_name)
{
  jconfirm('Удалить файл '+file_name+'?',
    function () {
        document.getElementById('del_file'+field_id).value = file_name;
        document.getElementById('edit_mode').value = 1;
        save_line();
      },
    function () { }
  );
}

function addline()
{
  document.getElementById('add_line').value = 1;
  save_line();
}

function delline(line_id)
{
  jconfirm('Удалить эту запись?',
    function () {
      document.getElementById('del_line').value = line_id;
      save_line();
    });
}

var bHandler = function() {
  $(this).parent().parent().find("b").css("color", "black");
  $(this).parent().parent().css("background-color", "#fff");
  $(this).unbind("change", bHandler);
}

var submit_form_started = 0;
var double_found = 0;
var uniq_fields = { 435: { 'type_field':'3', 'name_field':'Название' },  };

function save_line()
{
  if (submit_form_started) return; // Форма уже отправлена
  check_save_sublines = 0;
  if ($('.add_link_block'))
    {
      $('[id^="add_link_block"]').each(function(){
          if (this.style.display!='none')
            {
              $('[part^="add_link_field"]').each(function(){
                  if ((this.value!='' && this.value!='<br>') || (this.innerHTML!='' && this.innerHTML!='<br>'))
				  {
                    check_save_sublines = 1;
				  }
                });
            }
        });
      if (check_save_sublines)
        {
          jconfirm('Вы не сохранили значение в поле связи, данные для этого поля не сохранятся. Продолжить?',
                        function()
                        {
                          $('[class^="add_link_block"]').each(function(){
                              this.style.display='none';
                            });
                          save_line();
                        },
                          function()
                          {

                          });
          return false;
        }
    }

  // проверка на дубли в уникальных полях
  for (var id in uniq_fields)
    {
      ajax_uniq_search(id, uniq_fields[id].type_field, uniq_fields[id].name_field, false);
      if (double_found) return false;
    }

  var main_field; var edit_field; var value_field;
  var emptyFields = [];
  for (i=0; i<48; i++)
    {
      field_id = id_fields[i];
      if (document.getElementById('edit'+field_id))
        {
          main_field = document.getElementById('main'+field_id).value==1?true:false;
          edit_field = true;
          value_field = $('#value'+field_id).val() + (document.getElementById('add_file'+field_id)?document.getElementById('add_file'+field_id).value:'');
          type_field = all_fields[field_id]['type_field'];
          mult_value = all_fields[field_id]['mult_value'];
          if (main_field && edit_field && (value_field == '' || (value_field == 'null' && type_field == 4) || (value_field.replace(',','.') == 0 && (type_field==1 || type_field==5 || type_field==7|| type_field==14 || (type_field==4 && mult_value)))))
              var pushed = emptyFields.push(field_id);
          if ((type_field==6 || type_field==9) && check_upload_file(field_id, document.getElementById('add_file'+field_id))==false) return false;
        }
    }
  if (emptyFields.length > 0)
    {
      for (i=0; i<emptyFields.length; i++)
        {
          i_obj = document.getElementById('value' + emptyFields[i]);

          if (i_obj.getAttribute('type') == "hidden")
            {
              if ($(i_obj).next().attr('type') == "hidden")
                  i_obj = document.getElementById('add_file' + emptyFields[i]);
            }

          $("#field_edit" + emptyFields[i]).css("background-color", "#ffe0e0");
          $("#field_edit" + emptyFields[i] + " b").css("color", "red");

          if ($("#field_edit" + emptyFields[i]).parent().css("display") == "none")
            {
              p_obj = $("#field_edit" + emptyFields[i]).parent();
              p_id  = $(p_obj).attr("id");
              p_id  = String(p_id);
              s_gr  = p_id.replace("fgroup_edit", "");
              show_fgroup(s_gr);
            }

          $(i_obj).bind("change", bHandler);
        }
      jalert('Не все обязательные поля (помеченные звездочкой) были заполнены.');
      return false;
    }

  
  // Запоминаем текущий режим редактирования
  document.getElementById('cur_mode').value=cur_mode;

  // Пользовательские on_submit
  for (var key in edit_form_submits)
      {
        if (edit_form_submits[key]()==false) return false;
      }
  var e_form = document.getElementById('edit_form');
  var elems = e_form.getElementsByTagName('input');
  var correct = true;
  var pref = 'templ_fill_ok';
  for (var i=0; i<elems.length; i++) {
    if (elems[i].id.substr(0,pref.length) == pref) {
      id = elems[i].id.substr(pref.length);
      if (elems[i].value=='0') {
//      jalert("Текст:\n\r"+document.getElementById('value'+id).value+"\n\r не соответствует шаблону");
        if (document.getElementById('value'+id).value)
           {
            document.getElementById('value'+id).className += ' bg_err';
            correct = correct && false;
           }
      }
    }
  }
  clear_recheck_text();
  if (correct) {
    submit_form_started = 1;
    $('#edit_form').submit();
  }
  else {
    jalert('Поля, не соответствующие шаблону, подсвечены красным цветом');
    return false;

  }
}

// Список связанных таблиц
function relView() {
  if (document.getElementById("rel_tables_list").style.display == 'none')
    {
      $.cookie('rel_list_view42', 1, { expires: 30});
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("rel_tables_list").style.display = 'block'; // В IE без анимации.
    }
  else
    {
      $.cookie('rel_list_view42', null);
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("rel_tables_list").style.display = 'none'; // В IE без анимации.
    }

  if (navigator.appVersion.indexOf("MSIE")==-1) $("#rel_tables_list").slideToggle(); // Для нормальных браузеров.

  $(".sidebar_header").toggleClass("rel_block rel_none");
}

// Список всех связанных таблиц
function relsubView() {
  if (document.getElementById("rel_tables_sublist").style.display == 'none')
    {
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("rel_tables_sublist").style.display = 'block'; // В IE без анимации.
      document.getElementById("rel_tables_show").innerHTML = lang.Hide;
    }
  else
    {
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("rel_tables_sublist").style.display = 'none'; // В IE без анимации.
      document.getElementById("rel_tables_show").innerHTML = lang.Show_more;
    }

  if (navigator.appVersion.indexOf("MSIE")==-1) $('#rel_tables_sublist').slideToggle();
}

// Список кнопок
function buttonListView() {
  if (document.getElementById("secondary_buttons").style.display == 'none')
    {
      $.cookie('view_buttons42', 1, { expires: 30});
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("secondary_buttons").style.display = 'block'; // В IE без анимации.
    }
  else
    {
      $.cookie('view_buttons42', null);
      if (navigator.appVersion.indexOf("MSIE")!=-1) document.getElementById("secondary_buttons").style.display = 'none'; // В IE без анимации.
    }

  if (navigator.appVersion.indexOf("MSIE")==-1) $("#secondary_buttons").slideToggle(); // Для нормальных браузеров.

  $("#sec_buttons_toogle").toggleClass("additional_block additional_none");
}

var cur_mode = "view";
function switch_mode(mode)
{
  cur_mode=mode;
  if (mode=='edit')
    {
      // Возвращаем прежние значения полей
     if (document.getElementById("value3711")) document.getElementById("value3711").value=load_values[3711];
     if (document.getElementById("value435")) document.getElementById("value435").value=load_values[435];
     if (document.getElementById("value1047")) document.getElementById("value1047").value=load_values[1047];
     if (document.getElementById("value441")) document.getElementById("value441").value=load_values[441];
     if (document.getElementById("value442")) document.getElementById("value442").value=load_values[442];
     if (document.getElementById("value443")) document.getElementById("value443").value=load_values[443];
     if (document.getElementById("edit_value443")) document.getElementById("edit_value443").value=document.getElementById("edit_value443").getAttribute('first_value');
     if (document.getElementById("value438")) document.getElementById("value438").value=load_values[438];
     if (document.getElementById("value772")) document.getElementById("value772").value=load_values[772];
     if (document.getElementById("value444")) document.getElementById("value444").value=load_values[444];
     if (document.getElementById("value770")) document.getElementById("value770").value=load_values[770];
     if (document.getElementById("value445")) document.getElementById("value445").value=load_values[445];
     if (document.getElementById("value1460")) document.getElementById("value1460").value=load_values[1460];
     if (document.getElementById("value1470")) document.getElementById("value1470").value=load_values[1470];
     if (document.getElementById("value439")) document.getElementById("value439").value=load_values[439];
     if (document.getElementById("value440")) document.getElementById("value440").value=load_values[440];
     if (document.getElementById("value3340")) document.getElementById("value3340").value=load_values[3340];
     if (document.getElementById("value1056")) document.getElementById("value1056").value=load_values[1056];
     if (document.getElementById("value1057")) document.getElementById("value1057").value=load_values[1057];
     if (document.getElementById("value1065")) document.getElementById("value1065").value=load_values[1065];
     if (document.getElementById("value1061")) document.getElementById("value1061").value=load_values[1061];
     if (document.getElementById("value1058")) document.getElementById("value1058").value=load_values[1058];
     if (document.getElementById("value1059")) document.getElementById("value1059").value=load_values[1059];
     if (document.getElementById("value1060")) document.getElementById("value1060").value=load_values[1060];
     if (document.getElementById("value1062")) document.getElementById("value1062").value=load_values[1062];
     if (document.getElementById("value1063")) document.getElementById("value1063").value=load_values[1063];
     if (document.getElementById("value1064")) document.getElementById("value1064").value=load_values[1064];
     if (document.getElementById("value3810")) document.getElementById("value3810").value=load_values[3810];
     if (document.getElementById("value1088")) document.getElementById("value1088").value=load_values[1088];
     if (document.getElementById("edit_value1088")) document.getElementById("edit_value1088").value=document.getElementById("edit_value1088").getAttribute('first_value');

      document.getElementById('view_block').style.display = 'none';
      document.getElementById('view_buttons').style.display = 'none';
      document.getElementById('edit_block').style.display = 'block';
      document.getElementById('edit_buttons').style.display = 'block';
      $('[id^="fgroup_edit_header"]').each(function(key, group) {
        fgrpid = $(group).data('fgrpid');
        if($('#fgroup_edit'+fgrpid+' > tr').length == 0) {
          $(group).hide();
        }
      });
      $(".small_green_save").fadeIn();

      //возможность растягивать поля textarea по высоте и ширине
      $("textarea[id^='value']").each(function (i) {
         this.style.resize = 'both';
      });
    }
    else
    {
      document.getElementById('view_block').style.display = 'block';
      document.getElementById('view_buttons').style.display = 'block';
      document.getElementById('edit_block').style.display = 'none';
      document.getElementById('edit_buttons').style.display = 'none';
      $(".small_green_save").fadeOut();
    }
}

function display_field(field_id, show)
{
  if (show)
    {
      if (document.getElementById('field_view'+field_id)) document.getElementById('field_view'+field_id).style.display = '';
      if (document.getElementById('field_edit'+field_id)) document.getElementById('field_edit'+field_id).style.display = '';
    }
    else
    {
      if (document.getElementById('field_view'+field_id)) document.getElementById('field_view'+field_id).style.display = 'none';
      if (document.getElementById('field_edit'+field_id)) document.getElementById('field_edit'+field_id).style.display = 'none';
    }
}

function display_fgroup(fgroup_id, show)
{
  if (show)
    {
      if (document.getElementById('fgroup_view_header'+fgroup_id)) document.getElementById('fgroup_view_header'+fgroup_id).style.display = '';
      if (document.getElementById('fgroup_edit_header'+fgroup_id)) document.getElementById('fgroup_edit_header'+fgroup_id).style.display = '';
      if (document.getElementById('fg_img_view'+fgroup_id).src.slice(-9) == 'block.gif')
        {
          if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fgroup_view'+fgroup_id).style.display = '';
          if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fgroup_edit'+fgroup_id).style.display = '';
        }
    }
    else
    {
      if (document.getElementById('fgroup_view_header'+fgroup_id)) document.getElementById('fgroup_view_header'+fgroup_id).style.display = 'none';
      if (document.getElementById('fgroup_edit_header'+fgroup_id)) document.getElementById('fgroup_edit_header'+fgroup_id).style.display = 'none';
      if (document.getElementById('fg_img_view'+fgroup_id).src.slice(-9) == 'block.gif')
        {
          if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fgroup_view'+fgroup_id).style.display = 'none';
          if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fgroup_edit'+fgroup_id).style.display = 'none';
        }
    }
}

function show_fgroup(fgroup_id)
{
  if (document.getElementById('fgroup_view'+fgroup_id).style.display == 'none')
    {
      if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fgroup_view'+fgroup_id).style.display = '';
      if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fgroup_edit'+fgroup_id).style.display = '';
      if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fg_img_view'+fgroup_id).src = 'images/block.gif';
      if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fg_img_edit'+fgroup_id).src = 'images/block.gif';
      save_fgroup_set(fgroup_id, 1);
    }
    else
    {
      if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fgroup_view'+fgroup_id).style.display = 'none';
      if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fgroup_edit'+fgroup_id).style.display = 'none';
      if (document.getElementById('fgroup_view'+fgroup_id)) document.getElementById('fg_img_view'+fgroup_id).src = 'images/none.gif';
      if (document.getElementById('fgroup_edit'+fgroup_id)) document.getElementById('fg_img_edit'+fgroup_id).src = 'images/none.gif';
      save_fgroup_set(fgroup_id, 0);
    }
}
var click_status;
function save_fgroup_set(fgroup_id, vis)
{
    var req;
    if (window.XMLHttpRequest) {
        try {
            req = new XMLHttpRequest();
        } catch (e){}
    } else if (window.ActiveXObject) {
        try {
            req = new ActiveXObject('Msxml2.XMLHTTP');
        } catch (e){
            try {
                req = new ActiveXObject('Microsoft.XMLHTTP');
            } catch (e){}
        }
    }
    if (req) {
        req.open("GET", "save_fgroup_set.php?fgroup="+fgroup_id+"&vis="+vis, true);
        req.send(null);
    }
}

var prev_show_block=0;
function add_link_block_show(field_id)
{
  var v_o=document.getElementById('add_link_block'+field_id);
  var v_t;
  var v_e;
  var l_field;
  var l_field_id;
  if (v_o.style.display=='none')
     {
       $('.add_link_block'+field_id).css('display','');

       // Устанавливаем в отображаемое поле, значение которое забили в быстром поиске
       l_field_id = all_fields[field_id]['s_field_id'];
       l_field = all_fields[l_field_id];
       v_t = document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0');
       v_e = document.getElementById('edit_value'+field_id);
       if ((l_field['type_field']==1)||(l_field['type_field']==2)||(l_field['type_field']==3))
         { // Число, дата, строка
           if (v_e.getAttribute('first_value') != v_e.value)
              v_t.value=v_e.value;

           //очистка полей в зеленом плюсике
           //********************************************************************//
           if(click_status ===1)
           {
             var elem_arr = [];
             $("tr .add_link_block"+field_id+" > td[id*='sub_cell_'] > input[part='add_link_field']").each(function(){
               elem_arr[$(this).attr('field_id')]= $(this).attr('field_id');
             });
             for (var i=0;i<elem_arr.length;i++)
             {
               if(elem_arr[i])
               {
                 var l_field_id_1 = elem_arr[i];
                 var l_field_1 = all_fields[l_field_id_1];
                 var v_t_1 = document.getElementById('fast_edit_span_'+l_field_id_1+'_'+field_id+'_0');
                 var v_e_1 = document.getElementById('edit_value'+field_id);
                 if ((l_field_1['type_field']==1)||(l_field_1['type_field']==2)||(l_field_1['type_field']==3));
                   v_t_1.value="";
               }
             }
             click_status = 0;
           }
           //********************************************************************//
         }
     }
     else
     {
       $('.add_link_block'+field_id).css('display','none');
     }
  if ((prev_show_block)&&(prev_show_block!=field_id))
     {
       v_o=document.getElementById('add_link_block'+prev_show_block);
       $('.add_link_block'+field_id).css('display','none');
     }
 prev_show_block=field_id;
}

function html_form(str)
{
  str=str_replace('<br>',"\n",str);
  str=str_replace('<BR>',"\n",str);
  str=str_replace('</div><div>',"\n",str);
  str=str_replace('</DIV><DIV>',"\n",str);
  str=str_replace('<div>',"\n",str);
  str=str_replace('<DIV>',"\n",str);
  str=str_replace('</div>', "",str);
  str=str_replace('</DIV>', "",str);
  str=str_replace('&nbsp;',' ',str);
//  str=html_entity_decode($str,ENT_QUOTES,"UTF-8");
//  str=strip_tags($str);
  str=fulltrim(str);
  return str;
}

function add_link_block_save(field_id, add_link_field_ids)
{ // Сохранить результаты новой строки, и выбрать его в качестве значения для поля связи
  click_status = 1;
  var add_link_field_ids=explode(',', add_link_field_ids);
  var field;
  var l_field_id;
  var l_field;
  var v_o;
  var val;
  var update_params;
  field = all_fields[field_id];
  update_params="mfield="+field_id+"&sel=save&table="+field['s_table_id']+"&line=new&csrf=6RYQiq2uSijHG3PnKt5jPw==&";
  if (field['parent_link_field'])
     { // Поле фильтруется
       var parent_field_id=field['parent_link_field'];
       var parent_field=all_fields[parent_field_id];
       if (document.getElementById('value'+parent_field_id))
          {
            var parent_val = document.getElementById('value'+parent_field_id).value;
            // Также сохраняем его значение
            if (parent_field['s_table_id']!=field['s_table_id'])
                { // Новый тип полей фильтров связи
                  var filter_field_id=field['s_field_filter_id'];
                  update_params+="value[new]["+filter_field_id+"]="+parent_val+"&edit[new]["+filter_field_id+"]=1&"
                  if (parent_val) // значение вообще установлено
                     {
                       update_params+="value[new]["+filter_field_id+"]="+parent_val+"&edit[new]["+filter_field_id+"]=1&"
                     }
                     else
                     {
                        if (all_fields[filter_field_id]['main'])
                           {
                             jalert('Заполните пожалуйста поле: '+parent_field['name_field']);
                             return;
                           }
                     }
                }
          }
     }
  if (field['child_link_field']&&document.getElementById('edit_value'+c_l_f))
     {
       var c_l_f=field['child_link_field'];
       v_o=document.getElementById('edit_value'+c_l_f);
       v_o.value='';
       $(v_o).change();
       document.getElementById('value'+c_l_f).value='';
       document.getElementById('value'+c_l_f).onchange();
     }
  for (var i in add_link_field_ids)
    {
      l_field_id=add_link_field_ids[i];
      if (!l_field_id) continue;
      l_field = all_fields[l_field_id];
      v_o = document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0');
      if ((l_field['type_field']==1)||(l_field['type_field']==2)||(l_field['type_field']==3)||(l_field['type_field']==3))
         { // Число, дата, строка
           if (typeof(v_o.value)!=='undefined') val=v_o.value;
              else
           if (typeof(v_o.innerHTML)!=='undefined') val=v_o.innerHTML;
           val=html_form(val);
           if (!val && (l_field['main'] == 1 || l_field_id == explode('|',field['type_value'])[1]))
              {
                jalert("Заполните пожалуйста поле: "+l_field['name_field']);
                return ;
              }
           update_params+="value[new]["+l_field_id+"]="+encodeURIComponent(val)+"&edit[new]["+l_field_id+"]=1&";
         }
      if ((l_field["type_field"]==4)||(l_field["type_field"]==13)||
          (l_field["type_field"]==7)||(l_field["type_field"]==11))
         { // Список
           val=v_o.value;
           if (!val && (l_field['main'] == 1 || l_field_id == explode('|',field['type_value'])[1]))
              {
                jalert("Заполните пожалуйста поле: "+l_field['name_field']);
                return ;
              }
           update_params+="value[new]["+l_field_id+"]="+encodeURIComponent(val)+"&edit[new]["+l_field_id+"]=1&";
         }
      if (l_field["type_field"]==5)
         { // Связь
           val=v_o.getAttribute('f_value');
           if (!val && (l_field['main'] == 1 || l_field_id == explode('|',field['type_value'])[1]))
              {
                jalert("Заполните пожалуйста поле: "+l_field['name_field']);
                return ;
              }
           update_params+="value[new]["+l_field_id+"]="+encodeURIComponent(val)+"&edit[new]["+l_field_id+"]=1&";
         }
    }
  var correct = true;
	for (var i in add_link_field_ids)
    {
      l_field_id=add_link_field_ids[i];
	  if (!l_field_id) continue;
	  l_field = all_fields[l_field_id];
	   v_o = document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0');
        var pref = 'templ_fill_link_ok';
	    var temp_fill_link_ok = $(v_o).parent().find('#templ_fill_link_ok'+l_field_id);
		if(temp_fill_link_ok.length >0)
		{
		  var myelems = temp_fill_link_ok.get(0);
          if (myelems.id.substr(0,pref.length) == pref && v_o.value.toString().trim() != '')
	      {
            id = myelems.id.substr(pref.length);
            if (myelems.value=='0')
		    {
//            jalert("Текст:\n\r"+document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0').value+"\n\r не соответствует шаблону");
              if (document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0').value)
               {
                document.getElementById('fast_edit_span_'+l_field_id+'_'+field_id+'_0').className += ' bg_err';
                correct = correct && false;
               }
			   return ;
            }
          }
		}
   }
  if(correct)
  {
    $("input[id^='fast_edit_span_']").removeClass('bg_err');
	$("input[id^='fast_edit_span_']").removeClass('bg_fill');
  }
  clear_recheck_text();
  ajax_view_line.format=0;
  ajax_view_line.method="POST";
  ajax_view_line.call(update_params, function(resp){
    var res_arr=resp.toString().split("|");
    var act = res_arr[0];
    var table_id=res_arr[1];
    var line_id =res_arr[2];
    var new_value = Base64.decode(res_arr[3]);
    if (act=='dupl')
       {
          jalert(lang.fast_field_already_exists_p1+" \""+all_fields[new_value]['name_field']+"\" "+lang.fast_field_already_exists_p2);
                }
            else
                {
                    add_link_block_show(field_id);
                    v_o = document.getElementById('edit_value' + field_id);
                    v_o.value = new_value;
                    $(v_o).flushCache();
                    v_o = document.getElementById('value' + field_id);//=line_id;
                    v_o.value = line_id;
                }
            }
    )
    ;
    }

    /// ----------------------------------------------------------------
    ///
    ///                     БЫСТРОЕ РЕДАКТИРОВАНИЕ
    ///
    /// ----------------------------------------------------------------

    var fast_edit_old = "";
    var ajax_update = new XMLHTTP("update_value.php");
    var ajax_view_line = new XMLHTTP("view_line2.php");
    var last_tabindex_fast_edit =800;
    var is_shift_pressed = 0; // глобальное состояние, содежащие, нажат ли шифт
    var skip_fast_focus = 0; // не обрабатывать событие onfocus у редактируемого элемента
    var skip_fast_blur = 0;  // не обрабатывать событие onblur у редактируемого элемента

    // Массив содержащий описания полей
    var all_fields = {
