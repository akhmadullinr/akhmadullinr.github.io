                                <script>
                                    function select_all(param) {
                                        var first_elem = 1;
                                        var status;
                                        $("td #field_edit" + param + "_td1").children('input').each(function () {
                                            if (this.type == 'checkbox') {
                                                if (first_elem == 1) {
                                                    first_elem += 1;
                                                    if (this.checked)
                                                        status = 1;
                                                    else
                                                        status = 0;
                                                }
                                                else {
                                                    if (status == 1)
                                                        this.checked = true;
                                                    else
                                                        this.checked = false;

                                                    document.getElementById('value' + param).value++;
                                                    $('#value' + param).change();
                                                }
                                            }
                                        });
                                    }
                                </script>

