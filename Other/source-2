                                                                                        <script type="text/javascript">
                            $(document).ready(function load_3711() {

  if (document.getElementById("value3711") && document.getElementById("tab_1"))
  {
    if (document.getElementById("value3711").value=="Физ.лицо") {
      $("#tab_1").css("display","none");
    }
    else {
      $("#tab_1").css("display","");
    }
  }
  $("select[field_id='773'] option[value='Получение оплаты'], select[field_id='773'] option[value='Выставление счета']").hide();
  function hideSomeParams() {
    $("select[field_id='2960'] option[value!='Выставлен счет'][value!='Отказ']").hide();
  }
  if ($('#view_cell_552').html()=="Выставлен счет")
      hideSomeParams();
  $("select[field_id='2960']").on("change", function(){
    if ($(this).val()=='Выставлен счет')
      hideSomeParams();
  })
})

$(function(){
  
})

function onchange_3711()
{ // Функция по событию "изменение поля"
  // при смене вида лица введенные данные сохраняются и открываются на редактирование вновь
  document.getElementById('edit_mode').value=1;
  save_line();
}

addHandler(document.getElementById("value3711"), "onchange", onchange_3711);                        </script>
