        // Обрабатываем галочки
        document.getElementById('header_0_div').style.width = document.getElementById('header_0_div').offsetWidth + "px";
        document.getElementById('header_0_div').style.height = (document.getElementById('header_0').offsetHeight - 1) + "px";

        // Создаем div с фиксированной позицией
        // Переводим галочки у строк в данный div
        table_div = document.getElementById('table_div');
        /*
         table_div = document.createElement('div');

         table_div.className = '';
         table_div.id = 'table_div';
         table_div.style.position="fixed";
         */

        table_div.style.left = table_padding + "px"; // Задаем позицию такую же как у таблицы
        table_div.style.top = fields_content_table.offsetTop + "px";
        table_div.style.width = "";
        table_div.style.height = "";
//  document.getElementById('sel_records').appendChild(table_div); // Добавляем ее в форму, чтобы галочки были внутри формы

        var v_o, v_h, v_w, line_id;
        for (var i = 0; i < all_lines_pp.length; i++) {
            line_id = all_lines_pp[i];
            if (!line_id) continue;
            v_o = document.getElementById("chk_div_o_" + line_id);
            v_el = document.getElementById("chk_div" + line_id);
            v_h = document.getElementById("tr" + line_id).offsetHeight;
            if (!v_o)
                continue;
            v_w = v_o.offsetWidth;
            v_el.style.height = v_h + "px";
            document.getElementById("chk_div_t_" + line_id).style.height = v_h + "px";
            v_el.style.width = v_w + "px";
            v_el.style.top = (document.getElementById("td" + line_id + ".0").offsetTop) + "px";
            v_el.style.backgroundColor = document.getElementById("bg" + line_id).value;
            v_o.parentNode.removeChild(v_o);
        }

        
    }
    ;

    function fixWebkitImages() {
        var v_o, v_h, v_w, line_id;
        for (var i = 0; i < all_lines_pp.length; i++) {
            line_id = all_lines_pp[i];
            if (!line_id) continue;
            v_el = document.getElementById("chk_div" + line_id);
            v_o = document.getElementById("tr" + line_id).offsetHeight;
            if (!v_o)
                continue;
            v_w = v_o.offsetWidth;
            v_o = v_o - 1;
            v_el.style.height = v_o + "px";
            v_el.style.width = v_w + "px";
            v_el.style.top = (document.getElementById("td" + line_id + ".0").offsetTop) + "px";
        }

        for (var i = 0; i < all_lines_pp.length; i++) {
            line_id = all_lines_pp[i];
            if (!line_id) continue;
            v_el = document.getElementById("rp_div" + line_id);
            if (!v_el.style)
                continue;
            v_t = document.getElementById("tr" + line_id).offsetHeight - 1;
            v_el.style.height = v_t + "px";
            document.getElementById("rp_div_t" + line_id).style.height = v_t + "px";
            v_el.style.width = document.getElementById("rp_div" + line_id).offsetWidth + "px";
            v_el.style.left = (document.getElementById("td" + line_id + ".").offsetLeft - table_padding) + "px";
            v_el.style.top = (document.getElementById("td" + line_id + ".").offsetTop) + "px";
            v_el.style.position = "absolute";
            v_el.style.backgroundColor = document.getElementById("bg" + line_id).value;
            if (document.getElementById("td" + line_id + ".").style.background && document.getElementById("td" + line_id + ".").style.background != "transparent")
                v_el.style.background = document.getElementById("td" + line_id + ".").style.background;
            rp_div.appendChild(v_el.parentNode.removeChild(v_el));
        }
    }

    function on_line_resize(line_id, field_id) {
                return;
                var new_height, line_id;
        for (var i = 0; i < all_lines_pp.length; i++) {
            line_id = all_lines_pp[i];
            if (!line_id)
                continue;

            var td = document.getElementById("td" + line_id + ".0");
            new_height = td.offsetHeight - 1;
            $("#chk_div" + line_id).css({
                "top": td.offsetTop,
                "height": new_height
            });
            // Зафиксированное поле
            if (rp_div) {
                $("#rp_div" + line_id).css({
                    "top": td.offsetTop,
                    "height": new_height
                });
            }
        }
    }
    ;

    function rebuild_header() {
        var l_id = all_lines_pp[0];
        $('[id^=td' + l_id + ']').each(function (num, elem) {
            $('#header_' + elem.id.split('.')[1]).css({
                width: $(elem).width() + "px",
                minWidth: $(elem).width() + "px",
                maxWidth: $(elem).width() + "px"
            });
        });
    }

    function on_top_resize() {
                return;
                if (document.getElementById('top_header_back')) { // изменяем высоту подложки
            document.getElementById('top_header_back').style.height = top_header_div.offsetHeight + "px";
        }
        ;

        // Эммитация скрола, чтобы пересчитать высоту табличных эелементов
        old_offs_y = -1;
        header_orientir = -1;
        scroll_orientir = -1;
        onMyScroll();
    }

    function redraw_right_white() {
        if (fields_content_table) {
            right_white.style.left = (window_width - table_padding) + "px";
            right_white.style.top = "0px";
            right_white.style.width = table_padding + "px";
            right_white.style.height = window_height + "px";
            left_white.style.left = "0px";
            left_white.style.top = "0px";
            left_white.style.width = table_padding + "px";
            left_white.style.height = window_height + "px";
        }
        ;
    }

    var last_ie_width = 0;
    var last_ie_height = 0;
    function on_window_resize() {
                return;
        
        left_white = document.getElementById('left_white');
        right_white = document.getElementById('right_white');
        top_header_div = document.getElementById('top_header_div');
        fields_content_table = document.getElementById('fields_content_table');
        fields_buttons_table = document.getElementById('fields_buttons_table');
        popup_div_headers = document.getElementById('popup_div_headers')
        popup_table_headers = document.getElementById('popup_table_headers')
        window_width = document.documentElement.clientWidth;
        window_height = document.documentElement.clientHeight;
        buttons_h = fields_buttons_table.offsetHeight;

        if (page_loaded) {
            visible_popup = 0;
            on_top_resize();
            redraw_right_white();
        }
        else if (document.getElementById('top_header_back')) {  //если есть фоновый div блок
            top_header_div.style.position = 'absolute';
            document.getElementById('top_header_back').style.height = top_header_div.offsetHeight + "px";
            redraw_right_white();
        }
        if ((document.getElementById('footback_table_id')) && (fields_content_table)) { // фиксируем ширину подвала
            document.getElementById('footback_div_id').style.width = fields_content_table.offsetWidth + "px";
            document.getElementById('footback_table_id').style.width = window_width + "px";
        }
        rebuild_header();
    }

    var clear_header_cat_timer = 0;
    function ClearHeaderCat() {
                document.getElementById('cat' + prev_cat).style.display = 'none';
        document.getElementById('cat1').style.display = 'block';
        document.getElementById('active_cat_' + prev_cat).blur();
        prev_cat =1;
        clear_header_cat_timer = 0;
            }

