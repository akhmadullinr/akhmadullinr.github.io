<!-- Business_processes, dialog -->
<div id="dialog_bizproc" class="dialog1" style="display:none;">
    <div class="hr1"></div>
                                        <p>
                    <button class="d_button" id="true_bizproc"
                            onclick="location.href='bizproc.php?proc=1'">Знакомство с Клиентской базой</button>
                </p>
                                            </div>
<div id="dialog_bizproc_2" class="dialog1" style="display:none">
    <div class="hr1"></div>
    <div id="dialog_height">
        <form method="post">
            <table>
                <tr>
                    <img style="margin-top:20px;margin-bottom:20px;display:none" src=""
                         id="img_id">
                </tr>
                <tr>
                    <td id="desc_id"></td>
                </tr>
            </table>
            <table>
                <tr>
                    <div id="g_button"
                         style="display:none; width:220px; height:35px; margin-left:auto; margin-right:auto;margin-top:10px"
                         class="button save_prez"></div>
                </tr>
                <tr>
                    <div id="b_button"
                         style="display:none; width:220px; height:35px; margin-left:auto; margin-right:auto;margin-top:10px"
                         class="button addit_act"></div>
                </tr>
            </table>
            <input type="hidden" name="csrf" value="6RYQiq2uSijHG3PnKt5jPw=="/>
            <input type="hidden" id="img_width" name="img_width" value=""/>
            <input type="hidden" id="img_height" name="img_height" value=""/>
        </form>
    </div>
</div>
<script>
    var my_bizproc_flag = 0;
    var click_flag = 0;
    var my_bizproc_start;
    function count(obj) {
        var count = 0;
        for (var prs in obj) {
            if (obj.hasOwnProperty(prs)) count++;
        }
        return count;
    }

    function open_dialog_my(bizproc_id, i) {
        if (!i) i = 1;
        my_param_g = 1;
        my_param_b = 1;

        $.ajax({
            url: "bizproc.php",
            dataType: "json",
            data: { bizproc_id: bizproc_id, index_top_flag: 1 },
            success: function (data) {

                data_count = count(data);
                start_count = data_count;

                if (data[i].att_file_name != '') {
                    document.getElementById('img_id').style.display = 'block';
                    document.getElementById('img_width').value = data[i].width;

                    if (data[i].description.length > 800)
                        document.getElementById('img_height').value = data[i].height + 300;
                    else
                        document.getElementById('img_height').value = data[i].height;
                }

                if (data[i].att_file_name == '') {
                    document.getElementById('img_id').style.display = 'none';
                    if (data[i].description.length > 200)
                        document.getElementById('img_width').value = 600;
                    else
                        document.getElementById('img_width').value = 500;

                    if (data[i].description.length > 800)
                        document.getElementById('img_height').value = 600;
                    else
                        document.getElementById('img_height').value = data[i].height;
                }

                //if ($(".dialog1 ui-dialog-content ui-widget-content"))

                $("#dialog_bizproc").dialog('close');
                my_bizproc_start = 1;
                open_my_dialog();

                var parentElem = document.getElementById('dialog_bizproc_2');
                for (var iteration = 1; iteration < data_count + 1; iteration++) {
                    var div = document.createElement('div');
                    div.className = 'form_switcher';
                    div.id = 'my_id' + iteration;
                    if (iteration == 1)
                        div.style.backgroundPosition = 'left';
                    div.style.display = 'inline-block';
                    parentElem.appendChild(div);
                }

                //изначально скрываем кнопки
                document.getElementById('g_button').style.display = 'none';
                document.getElementById('b_button').style.display = 'none';

                if (data[i].g_button == '1') {
                    document.getElementById('g_button').style.display = 'block';
                    document.getElementById('g_button').innerHTML = data[i]['g_text'];
                }
                if (data[i].b_button == '1') {
                    document.getElementById('b_button').style.display = 'block';
                    document.getElementById('b_button').innerHTML = data[i]['b_text'];
                }


                $('#dialog_bizproc_2').click(function () {
                    if (click_flag)
                        return;
                    data_count -= 1;
                    i += 1;

                    //если все шаги прошли, закрываем диалог
                    if (i > start_count)
                        remove_page_arrows_my();

                    //изначально скрываем кнопки
                    document.getElementById('g_button').style.display = 'none';
                    document.getElementById('b_button').style.display = 'none';


                    document.getElementById('my_id' + i).style.backgroundPosition = 'left'

                    if (data_count > 0) {
                        //изначально скрываем кнопки
                        document.getElementById('g_button').style.display = 'none';
                        document.getElementById('b_button').style.display = 'none';

                        //задаем размер окна  с учетом картинкой
                        if (data[i].att_file_name != '') {
                            document.getElementById('img_id').style.display = 'block';
                            document.getElementById('img_width').value = data[i].width;

                            document.getElementById('img_height').value = data[i].height;
                            document.getElementById('dialog_height').style.height = '';
                        }

                        //пытаемся задать размеры окна без картинки
                        if (data[i].att_file_name == '') {
                            document.getElementById('img_id').style.display = 'none';

                            if (data[i].description.length > 200)
                                document.getElementById('img_height').value = 400;
                            else
                                document.getElementById('img_height').value = data[i].height;

                            if (data[i].description.length > 200)
                                document.getElementById('img_width').value = 600;
                            else
                                document.getElementById('img_width').value = 400;

                            document.getElementById('dialog_height').style.height = document.getElementById('img_height').value - 70 + 'px';
                        }


                        bp_name = data[i].name;
                        bp_desc = data[i].description;
                        bp_img = data[i].att_file;

                        open_my_dialog();

                        if (data[i].g_button == '1') {
                            document.getElementById('g_button').style.display = 'block';
                            document.getElementById('g_button').innerHTML = data[i]['g_text'];
                            my_param_g = i;
                        }
                        if (data[i].b_button == '1') {
                            document.getElementById('b_button').style.display = 'block';
                            document.getElementById('b_button').innerHTML = data[i]['b_text'];
                            my_param_b = i;
                        }

                        document.getElementById('bp_header_id').innerHTML = bp_name;
                        document.getElementById('desc_id').innerHTML = bp_desc;
                        document.getElementById('img_id').setAttribute('src', bp_img);
                    }

                });

                $('#g_button').click(function () {
                    if (document.getElementById('g_button').style.display == 'block' && data[i].g_link != '') {
                        my_bizproc_start = 0;
                        click_flag = 1;
                        $("#dialog_bizproc_2").dialog('close');
                        window.location.href = data[my_param_g].g_link;
                    }
                });

                $('#b_button').click(function () {
                    if (document.getElementById('b_button').style.display == 'block' && data[i].b_link != '') {
                        my_bizproc_start = 0;
                        click_flag = 1;
                        $("#dialog_bizproc_2").dialog('close');
                        window.location.href = data[my_param_b].b_link;
                    }
                });


                bp_name = data[i].name;
                bp_desc = data[i].description;
                bp_img = data[i].att_file;

                document.getElementById('bp_header_id').innerHTML = bp_name;
                document.getElementById('desc_id').innerHTML = bp_desc;
                document.getElementById('img_id').setAttribute('src', bp_img);
            }
        });
    }

    function remove_page_arrows_my() {
        //my_bizproc_flag = 1
        $("#dialog_bizproc").dialog('close');
        location.reload();
    }

    function open_my_dialog() {
        var obj = {};
        obj.modal = true;
        obj.title = '<table cellpadding="0" cellspacing="0"><tr><td height="10"><p id="bp_header_id" style="font-size:16px; font-weight:normal; color:#999; margin-top:3px; padding-left:8px;"></p></td><td></td></tr></table>';

        if (document.getElementById('img_width').value)
            obj.width = document.getElementById('img_width').value;
        obj.height = document.getElementById('img_height').value;

        //делаем точки в самом низу страницы
        //document.getElementById('dialog_height').style.height = obj.height -70 + 'px';
        obj.draggable = false;
        obj.resizable = false;
        $("#dialog_bizproc_2").dialog(obj);
        $("#dialog_bizproc_2").bind('dialogclose', remove_page_arrows_my);
        overlayHandler();
        $(window).on('resize', overlayHandler);
        $("div.ui-dialog").focus();
        return false;
    }

    function bizproc_view() {
        if (my_bizproc_start == 1)
            return;
        var obj = {};
        obj.modal = true;
        obj.title = '<table cellpadding="0" cellspacing="0"><tr><td height="10"><p style="font-size:16px; font-weight:normal; color:#999; margin-top:3px; padding-left:8px;">Бизнес-процессы</p></td><td><a style="text-decoration:none; " href="edit_bizproc.php?&back_url=NzE1NTIxZGI5ZjNkMjI4YzRiOGYwZDAxOTA2MmRmMWZ8L3ZpZXdfbGluZTIucGhwP3RhYmxlPTQyJmxpbmU9MiZmaWx0ZXI9NDgmc3VidGFibGU9OTcw"><img alt="" border="0" src="images/settings_a.gif" onmouseover="this.src=\'images/settings_b.gif\';" onmouseout="this.src=\'images/settings_a.gif\';" style="margin-left:10px; margin-top:-7px;"/></a></td></tr></table>';
        obj.width = 350;
        obj.draggable = false;
        obj.resizable = false;
        $("#dialog_bizproc").dialog(obj);
        overlayHandler();
        $("div.ui-dialog").focus();
        return false;
    }
</script>
<!-- End of business_processes -->



