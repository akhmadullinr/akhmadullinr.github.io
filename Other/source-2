    // Добавить запись в конец
    function sub_add_new_line(group_pp, top) {
        $(".small_green_save").fadeIn();
        var add_group_insrt_pararms = "";

        var t_html = document.getElementById('last_subtable_tr_' + cur_subtable_id).innerHTML;
        var t_id = 'subtable_' + cur_subtable_id + "_line_-" + last_inserted_lines_id;
        var el = document.createElement('div');
        var sumF = document.createElement('div');
        var sumFieldsContent = "";

        el.innerHTML = '<table><tr id="' + t_id + '">' + t_html + '</tr></table>'; // IE не позволяет менять innerHTML у TR всвязи с чем выстроено данное извращение
        var newTR = el.firstChild.rows[0];
        newTR.style.display = 'none';

        if ((group_pp == -1) || (group_pp == undefined)) { // Группа не задана добавляем в конец
            // Строка с суммами
            sumFieldsContent = $("#subtable_" + cur_subtable_id + "_sum_0").html();
            $("#subtable_" + cur_subtable_id + "_sum_0").remove();
            sumF.innerHTML = '<table><tr id="subtable_' + cur_subtable_id + '_sum_0">' + sumFieldsContent + '</tr></table>';
            var sumRow = sumF.firstChild.rows[0];
            if (top) {
                var subtable = $("#subtable_" + cur_subtable_id);
                $("tr:eq(0)", subtable).after(newTR);
            }
            else {
                document.getElementById('subtable_' + cur_subtable_id).appendChild(newTR.parentNode.removeChild(newTR));
            }
            document.getElementById('subtable_' + cur_subtable_id).appendChild(sumRow.parentNode.removeChild(sumRow));
        }
        else {
            delGroupLines[group_pp] = 0;
            // Строка с суммами
            sumFieldsContent = $("#subtable_" + cur_subtable_id + "_sum_" + group_pp).html();
            $("#subtable_" + cur_subtable_id + "_sum_" + group_pp).remove();
            sumF.innerHTML = '<table><tr id="subtable_' + cur_subtable_id + '_sum_' + group_pp + '">' + sumFieldsContent + '</tr></table>';
            var sumRow = sumF.firstChild.rows[0];

            var insert_point = document.getElementById('subtable_' + cur_subtable_id + '_group_' + group_pp);
            if (top) {
                $("#subtable_" + cur_subtable_id + "_top_group" + group_pp).after(newTR);
            }
            else {
                insert_point.parentNode.insertBefore(newTR, insert_point);
            }
            insert_point.parentNode.insertBefore(sumRow, insert_point);

            if (sub_groups_fields_defs[cur_subtable_id][group_pp] != undefined)
                add_group_insrt_pararms = "&def_value_f" + all_subtables[cur_subtable_id]['group_field'] + "=" + sub_groups_fields_defs[cur_subtable_id][group_pp];
        }
        newTR.style.background = '#fff6ad';

        var page_param = "";
        if (all_subtables[cur_subtable_id])
            page_param = "&subtable_page=" + all_subtables[cur_subtable_id]['cur_page'] + "&rel_field=" + all_subtables[cur_subtable_id]['rel_field'];

        ajax_update.format = 0;
        ajax_update.method = "POST";
        ajax_update.call("sel=insert_new_line&subtable_id=" + cur_subtable_id + "&csrf=6RYQiq2uSijHG3PnKt5jPw==" + "&parent_line_id=2&inserted_line=" + last_inserted_lines_id + add_group_insrt_pararms + page_param, ComRespInsrtSub);

        last_inserted_lines_id++;



                
        
    }

    function ComRespInsrtSub(resp) {
        resp = trim(resp, "\r\n");
        var resp_arr = resp.toString().split("\r\n");
        for (i = 0; i < resp_arr.length; i++) {
            var res_arr = resp_arr[i].toString().split("|");
            var instr_table_id = res_arr[1];
            var instr_line_id = res_arr[2];
            var cur_line_id = res_arr[3];
            if (trim(res_arr[0]) == "inserted") {
                var newTR = document.getElementById('subtable_' + cur_subtable_id + "_line_-" + cur_line_id);
                newTR.id = 'subtable_' + cur_subtable_id + "_line_" + instr_line_id;

                var n_html = newTR.innerHTML.replace(/_undefined_line_id_/g, instr_line_id);   // подменяем номер строки
                n_html = n_html.replace(/undefined_datepicker_class/g, 'datepicker'); // подменяем класс календаря
                n_html = n_html.replace(/undefined_fast_edit_select/g, 'fast_edit_select'); // подменяем класс селекта

                var t_id = newTR.id;
                var el = document.createElement('div');
                el.innerHTML = '<table><tr id="' + t_id + '">' + n_html + '</tr></table>'; // IE не позволяет менять innerHTML у TR всвязи с чем выстроено данное извращение
                var newTR2 = el.firstChild.rows[0];
                newTR2.style.background = '#fff6ad';
                newTR.parentNode.replaceChild(newTR2, newTR);

                $('input[type="text"]').each(function () {
                    $(this).attr('size', $(this).val().length);
                });

                //перевыставляем календарь на поля типа дата с отображением времени
                $('.datetime_textpad input').removeClass('datepicker');
                $('.datetime_textpad input').removeClass('hasDatepicker');
                $('.datetime_textpad').each(function (e) {
                    $(this).find("img.ui-datepicker-trigger").remove();
                });
                $('.datetime_textpad input').datetimepicker({
                    showOn: "button",
                    timeFormat: "HH:mm",
                    buttonImage: "images/calbtn.png",
                    buttonImageOnly: true,
                    buttonText: "Календарь",
                    showAnim: (('\v' == 'v') ? "" : "show"),  // в ie не включаем анимацию, тормозит
                });

                // Выставляем календарь на все невыставленные элементы
                $(".datepicker").datepicker({
                    showOn: "button",
                    buttonImage: "images/calbtn.png",
                    buttonImageOnly: true,
                    buttonText: "Календарь",
                    showAnim: (('\v' == 'v') ? "" : "show"),  // в ie не включаем анимацию, тормозит
                });

                $(".datepicker, .datetime_textpad input").parent().parent().css('white-space', 'nowrap');
                // Выставляем селект на все невыставленные элементы
                $('.fast_edit_select').each(form_fast_select);

                // выставляем редактирование поля связь
                /*
                 $(newTR2).find(".sub_edit_link_input").each(function(){
                 var f_id    =this.getAttribute("field_id");
                 var f_width =this.getAttribute("field_width");
                 var l_id=this.getAttribute("line_id");
                 //console.log("select_value.php?field="+f_id+"&line="+l_id);
                 $(this).f_autocomplete("select_value.php?field="+f_id+"&line="+l_id,
                 { width: f_width, scroll: true, scrollHeight: 300, minChars: 0, max: 1000, formatItem: formatItem, formatResult: formatResult }).result(sub_fix_lnk_result);
                 });
                 */

                // выполняем Javascript если он вписан
                var re = /<script>([\s\S]*?)<\/script>/gi;
                js_result = n_html.match(re);
                if (js_result) {
                    var o_str, s_len = String('<script>').length;
                    for (keyVar in js_result) {
                        if (intval(keyVar) != keyVar) continue; // Фикс для ie, т.к. могут быть не только числа
                        o_str = js_result[keyVar];
                        o_str = o_str.substr(s_len, o_str.length - s_len * 2 - 1);
                        eval(o_str);
                    }
                }
                // Обновляем если были вычисления
                UpdateData(resp_arr);

                // ставим фокус на первое поле
                var first_field_id = all_subtables[cur_subtable_id]['first_edit_field'];
                var first_field = document.getElementById("fast_edit_span_" + first_field_id + "_" + instr_line_id);
                if (first_field) {
                    if ((all_fields[first_field_id]['type_field'] == 1) || (all_fields[first_field_id]['type_field'] == 3)) {
                        var evt = { currentTarget: first_field };
                        onmousedown_text(evt); // FIX IE 8, некорректно работает привязка bind на новых объектах, поэтому используем addHandler
                    }
                    else $(first_field).mousedown()
                    $(first_field).focus();
                }

                setTimeout(function () {
                    newTR2.style.background = '';
                }, 100);
            }
        }

        update_css_param();
    }

    // удалить строку из подтаблицы
    function sub_drop_line(line_id) {
        var t_o = document.getElementById("subtable_" + cur_subtable_id + "_line_" + line_id);
        var page_param = "";
        if (all_subtables[cur_subtable_id])
            page_param = "&subtable_page=" + all_subtables[cur_subtable_id]['cur_page'] + "&rel_field=" + all_subtables[cur_subtable_id]['rel_field'];
        t_o.style.background = '#fff6ad';
        ajax_update.format = 0;
        ajax_update.method = "POST";
        ajax_update.call("sel=delete_line&table_id=" + all_subtables[cur_subtable_id]['table_id'] + "&line_id=" + line_id + "&csrf=6RYQiq2uSijHG3PnKt5jPw==" + page_param, ComRespDropSub);
    }
    ;

    var delGroupLines = new Object;

    function ComRespDropSub(resp) {
        resp = trim(resp, "\r\n")
        var resp_arr = resp.toString().split("\r\n");
        for (i = 0; i < resp_arr.length; i++) {
            var res_arr = resp_arr[i].toString().split("|");
            var drop_table_id = res_arr[1];
            var drop_line_id = res_arr[2];
            if (res_arr[0] == "cancel_delete") {
                var v_o = document.getElementById('subtable_' + cur_subtable_id + "_line_" + drop_line_id);
                v_o.style.background = '#ffadad';
                $(v_o).stop().animate({ backgroundColor: '#FFFFFF' }, 1500).css('background', '');
                // Обновляем, если были вычисления
                if (typeof resp_arr != 'undefined') UpdateData(resp_arr);
            }
            if (trim(res_arr[0]) == "deleted") {
                selTableInfo = all_subtables[cur_subtable_id]['table_id'];
                for (subtbl_id in all_subtables) {
                    if (selTableInfo != all_subtables[subtbl_id]['table_id']) continue;
                    var v_o = document.getElementById('subtable_' + subtbl_id + "_line_" + drop_line_id);
                    if (!v_o) continue;
                    // Убираем суммы, если в группе строк нет больше данных
                    var nextTr = $("#subtable_" + subtbl_id + "_line_" + drop_line_id + " + *").attr("id");
                    var prevTr = $("#subtable_" + subtbl_id + "_line_" + drop_line_id).prev().attr("id");
                    if (prevTr == undefined) prevTr = "";
                    if (nextTr.indexOf("subtable_" + subtbl_id + "_line_") < 0 && prevTr.indexOf("subtable_" + subtbl_id + "_line_") < 0) {
                        var grOrd = str_replace("subtable_3_sum_", "", nextTr);
                        delGroupLines[grOrd] = 1;
                        $("#" + nextTr + " div.sub_sum_gray").html("");
                        $("#" + nextTr + " div.sub_sum_page").html("");
                        if ($("[id^=subtable_" + subtbl_id + "_line]").size() == 1)
                            $("#" + nextTr + " div.sub_sum_black").html("");
                    }

                    if ((v_o) && (typeof(v_o.parentNode) !== 'undefined')) // Защита от быстрого двойного щелчка удалить
                        v_o.parentNode.removeChild(v_o);
                }

                if ($("#subtable_" + cur_subtable_id + " [id ^= subtable_" + cur_subtable_id + "_line_]").size() == 0 && cur_mode == "view") $(".small_green_save").fadeOut();
                // Обновляем, если были вычисления
                if (typeof resp_arr != 'undefined') UpdateData(resp_arr);
            }
        }
    }

    // редактировать строку из подтаблицы
    function sub_edit_line(line) {
        //location.href="";
    }
    ;

    function insertTextAtCursor(text, t_node) {
        var sel, range, html;
        if (window.getSelection) {
            sel = window.getSelection();
            if (sel.getRangeAt && sel.rangeCount) {
                range = sel.getRangeAt(0);
                range.insertNode(t_node);
                range.setEndAfter(t_node);
                range.setStartAfter(t_node);
                sel.removeAllRanges();
                sel.addRange(range);
            }
        } else if (document.selection && document.selection.createRange) {
            range = document.selection.createRange();
            range.pasteHTML(text);
            range.moveEnd('character', 0);
            range.select();

        }
    }

    // Вставляем стандартный перевод строки
    function insert_next_line(element) {
        insertTextAtCursor("<br>", document.createElement("br"));
    }

    function sub_save_click() {
        if (cur_mode == "edit") save_line();
        else $(".small_green_save").fadeOut();
    }

    function switch_subtable(subtable_id) {
        $(document.getElementById('tab_' + cur_subtable_id)).removeClass('subtabmenu_active');
        $(document.getElementById('tab_' + cur_subtable_id)).addClass('subtabmenu_noactive');

        $(document.getElementById('tab_' + subtable_id)).removeClass('subtabmenu_noactive');
        $(document.getElementById('tab_' + subtable_id)).addClass('subtabmenu_active');

        document.getElementById('sub_' + cur_subtable_id).style.display = 'none';
        document.getElementById('sub_' + subtable_id).style.display = 'block';

        $(document.getElementById('sub_' + subtable_id)).removeClass('subtable_div_hidden');
        $(document.getElementById('sub_' + cur_subtable_id)).addClass('subtable_div_hidden');

        document.getElementById('subtable').value = subtable_id;

        $("#subtable_to_table_link").attr('href', 'fields.php?table=' + all_subtables[subtable_id]['table_id'] + all_subtables[subtable_id]['status_line'] + '&rel_search&rel_table=42&rel_line=2&rel_field=' + all_subtables[subtable_id]['rel_field'] + (all_subtables[subtable_id]['filter_id'] != 0 ? '&filter=' + all_subtables[subtable_id]['filter_id'] : ''));
        cur_subtable_id = subtable_id;
        $.cookie("subtable_id", subtable_id);
        $("#setting_t").css('display','inline');
        if (cur_mode == "view") $(".small_green_save").css("display", "none");
        if(!all_subtables[subtable_id]['url']) {
            $("#setting_t").attr('href', 'edit_field.php?table=' + all_subtables[subtable_id]['table_id']);
        } else {
            if(all_subtables[subtable_id]['url'] === 'inact'){
                $("#setting_t").css('display','none');
            } else {
                $("#setting_t").attr('href',all_subtables[subtable_id]['url']);
            }
        }
    }

    $(function () {
        // уже понастоящемму скрываем подтаблицу
        $(".subtable_div_hidden").each(function () {

            this.style.display = 'none';
            this.className = '';
        });
    });
    
    function call_dialog(type, phones, table_id, line_id)
    {
        phones = phones.split(',');
        if (phones.length == 1) {
            var phone = phones[0];
            if (type == 'asterisk') {
                module_asterisk_call(phone, table_id, line_id);
            }
            if (type == 'asterisk_window') {
                module_asterisk_call_window(phone, table_id, line_id);
            }
        } else {
            var text = "";
            for (key in phones) {
                var phone = phones[key].replace(/ /g, '');
                if (phone != '') {
                    if (type == 'asterisk') {
                        var func = "module_asterisk_call('" + phone + "', " + table_id + ", " + line_id + ")";
                    }
                    if (type == 'asterisk_window') {
                        var func = "module_asterisk_call_window('" + phone + "', " + table_id + ", " + line_id + ")";
                    }
                    text += '<button class="al_button" onclick="' + func + '; $(\'.ui-dialog-titlebar-close\').click(); return false" style="margin-top:10px">' + phone + '</button>';
                }
            }
            jinfo("<div align=center>" + text + "</div>", "Выберите номер телефона", 280, true);
        }
    }

</script>
