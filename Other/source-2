<script>
    var sel_records = document.getElementById('sel_records');
    var global_cur_index = new Array();
    function show_template_preview(form_id, mode) {
        var newWindow = window.open("", "_blank");
        newWindow.blur();
        var checkValues = $("input[name='sel[]']:checked").map(function () {
            return $(this).val();
        }).get();
        if (checkValues == '') {
//      checkValues = $("input[name='sel[]']").map(function(){
//        return $(this).val();
//      }).get();
            checkValues = -1;
        }
        if (document.getElementById('view_line_id')) {
            checkValues = new Array();
            checkValues[0] = document.getElementById('view_line_id').value;
        }

        if (document.getElementById('array_status'))
            var array_status = document.getElementById('array_status').value;
        else
            var array_status = '';

        $.ajax({
            url: 'forms.php?ajax_sel_store&table_id=42',
            type: 'post',
            data: {
                sel: checkValues,
                array_status: array_status,
                csrf: '6RYQiq2uSijHG3PnKt5jPw=='
            },
            success: function () {
                newWindow.focus();
                newWindow.location.href = 'forms.php?table=42&mode=' + mode + '&form=' + form_id + '&view';
            }
        });
    }
    function remove_view_form(form_id, mode) {
        $("p[name='form_" + form_id + "']").remove();
        switch_form(global_cur_index[mode], mode);
    }

    function switch_form(cur_index, mode) {
        var template_count = $("#form_" + mode + "_wrapper p").size();

        if (cur_index >= $("#form_" + mode + "_wrapper p").size()) {
            cur_index = cur_index - 8;
        }
        global_cur_index[mode] = cur_index;
        $("#dialog_left_arrow_" + mode).click(function () {
            switch_form(cur_index - 8, mode);
        });
        $("#dialog_right_arrow_" + mode).click(function () {
            switch_form(cur_index + 8, mode);
        });
        if (cur_index == 0) {
            $("#dialog_left_arrow_" + mode).hide();
        } else {
            $("#dialog_left_arrow_" + mode).show();
        }
        if (cur_index + 8 > $("#form_" + mode + "_wrapper p").size()) {
            $("#dialog_right_arrow_" + mode).hide();
        } else {
            $("#dialog_right_arrow_" + mode).show();
        }

        $(".form_" + mode + "_switcher").css('background-position', 'right');
        $("#form_" + mode + "_switcher_" + cur_index).css('background-position', 'left');
        $("." + mode + "_btn").hide();
        for (var i = cur_index; i < cur_index + 8; i++) {
            $("#form_" + mode + "_wrapper p:eq(" + i + ")").show();
        }
        $(".form_" + mode + "_switcher").hide();
        if ($("#form_" + mode + "_wrapper p").size() > 8) {
            var j = 0;
            for (var i = 0; i < $("#form_" + mode + "_wrapper p").size(); i = i + 8) {
                $("#form_switcher_wrapper_" + mode + " button:eq(" + j + ")").show();
                j++;
            }
        }
        remove_page_arrows_send = function () {
            switch_form(0, 'send');
        }
        remove_page_arrows_sms = function () {
            switch_form(0, 'sms');
        }
        if (template_count > 0) {
            if (template_count < 9)
                $("#form_" + mode + "_wrapper").css('min-height', 53 * template_count + 'px');
            else {
                $("#form_" + mode + "_wrapper").css('min-height', '55px');
                $("#form_" + mode + "_wrapper").append(new_elem);
            }
        }
        else {
            $("#form_" + mode + "_wrapper").css('min-height', '55px');
            var new_elem = "<p class='add1'>Шаблоны не найдены, перейдите к <a href='#' onclick=\"document.getElementById('sel_records').submit(); return false;\">дополнительно</a> </p>";
            $("#form_" + mode + "_wrapper").append(new_elem);
        }
    }
</script>
