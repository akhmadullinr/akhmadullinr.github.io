    function check_new_values() {
                                                                    if (document.getElementById("value3711"))
            if (document.getElementById("value3711").value != load_values[3711]) {
                return jconfirm("Внимание! Вы изменили поле \"Вид\".\nУверены что не хотите сохранить данные?",
